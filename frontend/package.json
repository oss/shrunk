{
  "name": "shrunk",
  "version": "2.2.1",
  "description": "Rutgers University URL Shortener",
  "scripts": {
    "build": "node scripts/fix-antd-table.js && parcel build --no-source-maps --no-content-hash --public-url=../static/dist/ --dist-dir=../backend/shrunk/static/dist src/index.html src/linkhub-loader.html",
    "watch": "parcel watch --public-url=../static/dist/ --dist-dir=../backend/shrunk/static/dist src/index.html src/linkhub-loader.html",
    "doc": "node ./typedocRunner.ts",
    "lint": "eslint ./src --ext .ts,tsx --ignore-path ../.gitignore",
    "lint-fix": "eslint ./src --ext .ts,tsx --fix --ignore-path ../.gitignore",
    "format": "prettier --write . --config ./.prettierrc.js --ignore-path ../.gitignore"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@highcharts/map-collection": "^1.1.3",
    "animate.css": "^4.1.1",
    "animate.css-react": "^1.1.0",
    "antd": "^4.15.6",
    "csv-writer": "^1.6.0",
    "fbjs": "^3.0.0",
    "hi-base32": "^0.5.0",
    "highcharts": "^9.0.0",
    "highcharts-react-official": "^3.0.0",
    "history": "^5.0.0",
    "js-cookie": "^2.2.1",
    "moment": "^2.29.1",
    "parcel": "^2.7.0",
    "qrcode.react": "^1.0.0",
    "react": "^16.13.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.13.1",
    "react-icons": "^4.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "socket.io-client": "^3.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@parcel/transformer-less": "^2.7.0",
    "@types/js-cookie": "^2.2.6",
    "@types/node": "^14.11.5",
    "@types/qrcode.react": "^1.0.1",
    "@types/react": "^16.9.51",
    "@types/react-copy-to-clipboard": "^4.3.0",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.4.0",
    "babel-plugin-import": "^1.13.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "prettier": "^2.3.0",
    "process": "^0.11.10",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  },
