{
  "name": "shrunk",
  "version": "2.2.1",
  "description": "Rutgers University URL Shortener",
  "scripts": {
    "build": "parcel build --no-source-maps --no-content-hash --public-url=../static/dist/ --dist-dir=../backend/shrunk/static/dist src/index.html src/linkhub-loader.html",
    "watch": "parcel watch --public-url=../static/dist/ --dist-dir=../backend/shrunk/static/dist src/index.html src/linkhub-loader.html",
    "lint": "eslint ./src --ext .ts,tsx --ignore-path ../.gitignore",
    "lint-fix": "eslint ./src --ext .ts,tsx --fix --ignore-path ../.gitignore",
    "format": "prettier --write . --config ./.prettierrc.js --ignore-path ../.gitignore"
  },
  "dependencies": {
    "@ant-design/colors": "^7.2.0",
    "@ant-design/icons": "^4.6.2",
    "@highcharts/map-collection": "^1.1.3",
    "antd": "^5.21.2",
    "csv-writer": "^1.6.0",
    "dayjs": "^1.11.13",
    "fuse.js": "^7.1.0",
    "hi-base32": "^0.5.0",
    "highcharts": "^9.0.0",
    "highcharts-react-official": "^3.0.0",
    "history": "^5.0.0",
    "js-cookie": "^2.2.1",
    "parcel": "^2.13.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@types/js-cookie": "^2.2.6",
    "@types/node": "^14.11.5",
    "@types/react": "^16.9.51",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unused-imports": "^1.1.0",
    "prettier": "^2.3.0",
    "process": "^0.11.10",
    "typescript": "^4.2.4"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  },
  "author": "Rutgers Open System Solutions",
  "license": "MIT"
}
