{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "strict": true,
    "module": "esnext",
    "target": "es6",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "lib": ["dom"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    }
  }
}
